   "source": [
    "import chess\n",
    "import chess.svg\n",
    "import random\n",
    "from IPython.display import SVG, display\n",
    "from pgn_converter import convert\n",
    "board_moves = []\n",
    "board = chess.Board(\"rnbqkbnr/pppppppp/8/8/8/8/PPPPPPPP/RNBQKBNR w KQkq - 0 1\")\n",
    "while board.is_fifty_moves() == False and board.outcome() == None:\n",
    "    arr = []\n",
    "    board_legal_moves = list(board.legal_moves)\n",
    "    board_legal_moves = [str(i) for i in board_legal_moves]\n",
    "    for i in board_legal_moves:\n",
    "        arr.append(i[-6:])\n",
    "    move = random.choice(arr)\n",
    "    board.push_san(move)\n",
    "    board_moves.append(move)\n",
    "\n",
    "print(convert(board_moves))\n",
    "print(board.outcome)\n",
    "display(SVG(chess.svg.board(board=board)))\n",
    "\n"
   ]
  }
